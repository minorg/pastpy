<nav aria-label="Page navigation">
    <ul class="pagination justify-content-center ">
        <li class="page-item{{^pagination.previous_page_number.enabled}} disabled{{/pagination.previous_page_number.enabled}} ">
            <a class="page-link " href="{{pagination.previous_page_number.number}}.html " tabindex="-1 ">Previous</a>
        </li>
        {{#pagination.visible_page_numbers}}
        <li class="page-item{{#active}} active{{/active}} ">
            <a class="page-link " href="{{number}}.html ">{{number}}</a>
        </li>
        {{/pagination.visible_page_numbers}}
        <li class="page-item{{^pagination.next_page_number.enabled}} disabled{{/pagination.next_page_number.enabled}} ">
            <a class="page-link " href="{{pagination.next_page_number.number}}.html ">Next</a>
        </li>
    </ul>
</nav>
